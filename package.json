{
  "name": "csharp-interpolated-string-converter",
  "displayName": "Csharp Interpolated String Converter",
  "description": "Prepends a `$` to a string when a $ and { are entered within quotes in CSharp files (and removes the typed `$`)",
  "version": "0.1.1",
  "engines": {
    "vscode": "^1.49.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:csharp"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "publisher": "corylulu",
  "contributes": {
    "configuration": [
      {
        "title": "Csharp Interpolated String Converter",
        "properties": {
          "template-string-converter.quoteType": {
            "type": "string",
            "description": "quote type that gets replaced",
            "default": "double",
            "enum": [
              "single",
              "double",
              "both"
            ]
          },
          "template-string-converter.enabled": {
            "type": "boolean",
            "description": "extension is enabled",
            "default": true
          },
          "template-string-converter.validLanguages": {
            "type": "array",
            "description": "extension runs on these languages",
            "default": [
              "csharp"
            ]
          }
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corylulu/csharp-interpolated-string-converter"
  },
  "bugs": {
    "url": "https://github.com/corylulu/csharp-interpolated-string-converter/issues"
  },
  "devDependencies": {
    "@types/vscode": "^1.49.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "eslint": "^7.6.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "icon": "src/logo.png",
  "keywords": [
    "csharp",
    "interpolate",
    "interpolation",
    "template",
    "string",
    "converter",
    "auto",
    "embed",
    "embedder",
    "autocorrect"
  ]
}
